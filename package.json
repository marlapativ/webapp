  "scripts": {
    "clean:logs": "rm -rf logs",
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc -p tsconfig.production.json",
    "postbuild": "cp package*.json dist && cd dist && npm ci --production",
    "dev": "tsc --watch & NODE_ENV=development nodemon build/src/server",
    "start": "NODE_ENV=production pm2 start server --name server",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix",
    "test": "mocha --exit --require ts-node/register --require tests/utils/fixtures.ts",
    "test:watch": "mocha --watch",
    "coverage": "nyc --all --reporter=text npm run test",
    "tar": "",
    "zip": "rm -f $npm_package_config_archivename && zip -r $npm_package_config_archivename . $npm_package_config_archiveargs"
  },
